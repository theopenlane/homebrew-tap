{
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": [
        "^Formula/.*\\.rb$"
      ],
      "matchStrings": [
        "url \".*?/v(?<currentValue>\\d+\\.\\d+\\.\\d+)\""
      ],
      "depNameTemplate": "openlane",
      "datasourceTemplate": "github-releases",
      "lookupNameTemplate": "theopenlane/core"
    },
    {
      "fileMatch": [
        "^Formula/.*\\.rb$"
      ],
      "matchStrings": [
        "sha256 \"(?<currentValue>[a-f0-9]{64})\""
      ],
      "depNameTemplate": "openlane",
      "datasourceTemplate": "github-releases",
      "lookupNameTemplate": "theopenlane/core"
    }
  ]
}