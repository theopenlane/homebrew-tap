{
  "enabledManagers": [
    "custom.regex"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "^Formula/openlane\\.rb$"
      ],
      "matchStrings": [
        "url\\s+\"(?<url>https://github.com/theopenlane/core/releases/download/v(?<version>[\\d.]+)/openlane_[\\d.]+_(?<os>[a-z]+)_(?<arch>[a-z0-9]+)\\.tar\\.gz)\"\\s+sha256\\s+\"(?<sha256>[a-f0-9]{64})\""
      ],
      "datasourceTemplate": "github-releases",
      "versioningTemplate": "semver",
      "depNameTemplate": "openlane_{{ os }}_{{ arch }}.tar.gz",
      "currentValueTemplate": "{{ version }}",
      "packageNameTemplate": "theopenlane/core"
    },
    {
      "customType": "regex",
      "fileMatch": [
        "^Formula/openlane\\.rb$"
      ],
      "matchStrings": [
        "version\\s+\"(?<version>[\\d.]+)\""
      ],
      "datasourceTemplate": "github-tags",
      "versioningTemplate": "semver",
      "depNameTemplate": "openlane_version",
      "currentValueTemplate": "{{ version }}",
      "packageNameTemplate": "theopenlane/core"
    }
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "github-releases",
        "github-tags"
      ],
      "groupName": "Openlane CLI release assets"
    }
  ]
}