{
  "enabledManagers": [
    "custom.regex"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "^Formula/openlane\\.rb$"
      ],
      "matchStrings": [
        "url\\s+\"https://github.com/theopenlane/core/releases/download/v([\\d.]+)/openlane_[\\d.]+_([a-z]+)_([a-z0-9]+)\\.tar\\.gz\"\\s+sha256\\s+\"([a-f0-9]{64})\""
      ],
      "datasourceTemplate": "github-release-attachments",
      "versioningTemplate": "semver",
      "depNameTemplate": "openlane_{{ match2 }}_{{ match3 }}.tar.gz",
      "currentValueTemplate": "{{ match1 }}"
    }
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "github-release-attachments"
      ],
      "groupName": "Openlane CLI release assets"
    }
  ]
}