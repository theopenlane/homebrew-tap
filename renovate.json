{
  "regexManagers": [
    {
      "fileMatch": [
        "^Formula/.*\\.rb$"
      ],
      "matchStrings": [
        "url\\s+\"https://github\\.com/theopenlane/core/archive/refs/tags/v(?<currentValue>\\d+\\.\\d+\\.\\d+)\\.tar\\.gz\""
      ],
      "depNameTemplate": "openlane",
      "datasourceTemplate": "github-tags",
      "lookupNameTemplate": "theopenlane/core"
    },
    {
      "fileMatch": [
        "^Formula/.*\\.rb$"
      ],
      "matchStrings": [
        "sha256\\s+\"(?<currentValue>[a-f0-9]{64})\""
      ],
      "depNameTemplate": "openlane",
      "datasourceTemplate": "github-tags",
      "lookupNameTemplate": "theopenlane/core"
    }
  ]
}